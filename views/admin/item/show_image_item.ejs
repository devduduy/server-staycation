<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <div class="row">
    <% for ( let x = 0; x < item.imageId.length; x++ ) { %>
    <div class="col-lg-6 col-md-4 col-12">
      <div class="card shadow mb-4 mt-1">
        <div class="card-body">
          <img
            src="http://localhost:3001/<%= item.imageId[x].imageUrl %>"
            alt="image"
            class="img-fluid"
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a href="/admin/item" class="btn btn-secondary"
    ><i class="fas fa-backward"> Back</i></a
  >
</div>
